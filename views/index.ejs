<!doctype html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Favicon -->
    <link rel="shortcut icon" href="/favicon.png">

    <!-- Font -->
    <link href="https://fonts.googleapis.com/css2?family=Jost:wght@400;500;600;700&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Fredoka+One&family=Lora:wght@400;700&family=Montserrat:wght@400;500;600;700&family=Nunito:wght@400;700&display=swap" rel="stylesheet">

    <!-- Libs CSS -->
    <link rel="stylesheet" href="/fonts/fontawesome/fontawesome.css">
    <link rel="stylesheet" href="/libs/@fancyapps/fancybox/dist/jquery.fancybox.min.css">
    <link rel="stylesheet" href="/libs/aos/dist/aos.css">
    <link rel="stylesheet" href="/libs/choices.js/public/assets/styles/choices.min.css">
    <link rel="stylesheet" href="/libs/flickity-fade/flickity-fade.css">
    <link rel="stylesheet" href="/libs/flickity/dist/flickity.min.css">
    <link rel="stylesheet" href="/libs/highlightjs/styles/vs2015.css">
    <link rel="stylesheet" href="/libs/jarallax/dist/jarallax.css">
    <link rel="stylesheet" href="/libs/quill/dist/quill.core.css" />

    <!-- Map -->
    <link href='https://api.mapbox.com/mapbox-gl-js/v0.53.0/mapbox-gl.css' rel='stylesheet' />

    <!-- Theme CSS -->
    <link rel="stylesheet" href="/css/theme.min.css">

    <title>LearnFastTrack</title>

</head>
<body>

    <!-- NAVBAR
    ================================================== -->
	<%- include('partials/header', { categories: categories, topics: topics, tutorials: tutorials}) %>


    <!-- HERO
    ================================================== -->
    <section class="py-8 pt-xl-12 mt-n4 pb-lg-8 bg-shark bg-cover position-relative">

        <div class="container">
            <div class="row align-items-center">
                <div class="col-12 col-md-5 col-lg-6 order-md-2" data-aos="fade-in">

                    <!-- Image -->
                    <img src="img/illustrations/illustration-4.png" class="img-fluid ms-xl-5 mw-md-150 mw-lg-130 mb-6 mb-md-0" alt="...">

                </div>
                <div class="col-12 col-md-7 col-lg-6 order-md-1">
                    <!-- Heading -->
                    <h3 class="display-4 text-white mb-6" data-aos="fade-left" data-aos-duration="150">
                        Accelerate <span class="display-4 text-orange fw-bold">your learning</span>
                    </h3>

                    <!-- Form -->
                    <form action="/tutorials/search" method="get" class="mb-6 me-xl-9" data-aos="fade-up" data-aos-duration="200">
                        <div class="input-group">
                            
                            <input type="search" class="form-control ps-2 border-right-0 rounded-left-xl border-0" placeholder="What you will learn today" name="q">
                            <div class="input-group-append">
                                <!-- <span class="input-group-text border-right-0 text-primary icon-xs rounded-left-xl"></span> -->
                                <button class="input-group-text border-left-0 text-primary icon-xs rounded-right-xl" type="submit">
                                    <!-- Icon -->
                                    <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <path d="M8.80758 0C3.95121 0 0 3.95121 0 8.80758C0 13.6642 3.95121 17.6152 8.80758 17.6152C13.6642 17.6152 17.6152 13.6642 17.6152 8.80758C17.6152 3.95121 13.6642 0 8.80758 0ZM8.80758 15.9892C4.8477 15.9892 1.62602 12.7675 1.62602 8.80762C1.62602 4.84773 4.8477 1.62602 8.80758 1.62602C12.7675 1.62602 15.9891 4.8477 15.9891 8.80758C15.9891 12.7675 12.7675 15.9892 8.80758 15.9892Z" fill="currentColor"/>
                                        <path d="M19.762 18.6121L15.1007 13.9509C14.7831 13.6332 14.2687 13.6332 13.9511 13.9509C13.6335 14.2682 13.6335 14.7831 13.9511 15.1005L18.6124 19.7617C18.7712 19.9205 18.9791 19.9999 19.1872 19.9999C19.395 19.9999 19.6032 19.9205 19.762 19.7617C20.0796 19.4444 20.0796 18.9295 19.762 18.6121Z" fill="currentColor"/>
                                    </svg>
                                </button>
                            </div>
                        </div>
                    </form>

                </div>
            </div> <!-- / .row -->
        </div> <!-- / .container -->
    </section>

    
    <!-- CATEGORIES
    ================================================== -->
    <section class="py-4 py-md-9 bg-white">
        <div class="container">
            <div class="row align-items-end mb-md-8 mb-4">
                <div class="col-md mb-4 mb-md-0 text-center">
                    <h1 class="mb-1">Popular Topics</h1>
                </div>
            </div>

            <div class="row row-cols-md-3 row-cols-lg-5 row-cols-xl-6">
                <% topics.slice(0,10).forEach(topic => { %>
                    <div class="col-md mb-md-6 mb-4 px-2">
                        <!-- Card -->
                        <a href="/tutorials/topic/<%= topic.name %>" class="card card-border-hover border icon-category icon-category-sm p-5 lift shadow-dark-hover">
                            <!-- Image -->
                            <div class="row-cols-1 align-items-center justify-content-center mx-n3">
                                <div class="col-auto px-3">
                                    <div class="icon-h-p secondary text-center">
                                        <img href="<%= topic.image %>">
                                    </div>
                                </div>

                                <div class="col px-3">
                                    <!-- Body -->
                                    <div class="card-body p-0">
                                        <h6 class="mb-0 line-clamp-1 text-center"><%= topic.title %></h6>
                                        <p class="mb-0 line-clamp-1 text-center"><%= topic.description %></p>
                                    </div>
                                </div>
                            </div>
                        </a>
                    </div>
                <% }) %>

            </div>

            <div class="row align-items-end justify-content-center mb-md-8 mb-4">
                <div class="col-md-auto text-center">
                    <a href="/topics" class="d-flex align-items-center fw-medium">
                        Browse All
                        <div class="ms-2 d-flex">
                            <!-- Icon -->
                            <svg width="10" height="10" viewBox="0 0 10 10" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <path d="M7.7779 4.6098L3.32777 0.159755C3.22485 0.0567475 3.08745 0 2.94095 0C2.79445 0 2.65705 0.0567475 2.55412 0.159755L2.2264 0.487394C2.01315 0.700889 2.01315 1.04788 2.2264 1.26105L5.96328 4.99793L2.22225 8.73895C2.11933 8.84196 2.0625 8.97928 2.0625 9.1257C2.0625 9.27228 2.11933 9.4096 2.22225 9.51269L2.54998 9.84025C2.65298 9.94325 2.7903 10 2.9368 10C3.0833 10 3.2207 9.94325 3.32363 9.84025L7.7779 5.38614C7.88107 5.2828 7.93774 5.14484 7.93741 4.99817C7.93774 4.85094 7.88107 4.71305 7.7779 4.6098Z" fill="currentColor"/>
                            </svg>

                        </div>
                    </a>
                </div>
            </div>

        </div>
    </section>

    <!-- FEATURED TUTORIALS
    ================================================== -->
    <section class="pt-5 pb-9 pt-md-9 bg-dark bg-cover">
        <div class="container">
            <div class="text-center mb-5 mb-md-8">
                <h1 class="text-white mb-1">Featured Tutorials</h1>
            </div>

            <div class="mx-n4 flickity-button-outset" data-flickity='{"pageDots": false, "prevNextButtons": true, "cellAlign": "left", "wrapAround": true, "imagesLoaded": true}'>
                <% tutorials.slice(0,10).forEach(tutorial => { %>
                    <div class="col-12 col-md-6 col-xl-4 pb-4 pb-md-7" style="padding-right:15px;padding-left:15px;">
                        <!-- Card -->
                        <div class="card border rounded-xl shadow p-2 sk-fade">
                            <!-- Image -->
                            <div class="card-zoom position-relative">
                                <a href="/tutorials/<%= tutorial.name %>" class="card-img sk-thumbnail d-block">
                                    <img class="rounded shadow-light-lg" src="<%= tutorial.image %>" alt="<%= tutorial.title %>">
                                </a>
                            </div>

                            <!-- Footer -->
                            <div class="card-footer px-2 pb-2 mb-1 pt-4 position-relative">
                                <!-- Preheading -->
                                <a href="/tutorials/topic/<%= tutorial.topic %>"><span class="mb-1 d-inline-block text-gray-800"><%= tutorial.topic %></span></a>

                                <!-- Heading -->
                                <div class="position-relative">
                                    <a href="/tutorials/<%= tutorial.name %>" class="d-block stretched-link"><h4 class="line-clamp-2 h-md-48 h-lg-58 me-md-6 me-lg-10 me-xl-4 mb-2"><%= tutorial.title %></h4></a>

                                    <div class="row mx-n2 align-items-end">
                                        <div class="col px-2">
                                            <ul class="nav mx-n3">
                                                <li class="nav-item px-3">
                                                    <div class="d-flex align-items-center">
                                                        <div class="me-2 d-flex icon-uxs text-secondary">
                                                            <!-- Icon -->
                                                            <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                                                                <path d="M17.1947 7.06802L14.6315 7.9985C14.2476 7.31186 13.712 6.71921 13.0544 6.25992C12.8525 6.11877 12.6421 5.99365 12.4252 5.88303C13.0586 5.25955 13.452 4.39255 13.452 3.43521C13.452 1.54098 11.9124 -1.90735e-06 10.0197 -1.90735e-06C8.12714 -1.90735e-06 6.58738 1.54098 6.58738 3.43521C6.58738 4.39255 6.98075 5.25955 7.61414 5.88303C7.39731 5.99365 7.1869 6.11877 6.98502 6.25992C6.32752 6.71921 5.79178 7.31186 5.40787 7.9985L2.8447 7.06802C2.33612 6.88339 1.79688 7.26044 1.79688 7.80243V16.5178C1.79688 16.8465 2.00256 17.14 2.31155 17.2522L9.75312 19.9536C9.93073 20.018 10.1227 20.0128 10.2863 19.9536L17.7278 17.2522C18.0368 17.14 18.2425 16.8465 18.2425 16.5178V7.80243C18.2425 7.26135 17.704 6.88309 17.1947 7.06802ZM10.0197 1.5625C11.0507 1.5625 11.8895 2.40265 11.8895 3.43521C11.8895 4.46777 11.0507 5.30792 10.0197 5.30792C8.98866 5.30792 8.14988 4.46777 8.14988 3.43521C8.14988 2.40265 8.98866 1.5625 10.0197 1.5625ZM9.23844 18.1044L3.35938 15.9703V8.91724L9.23844 11.0513V18.1044ZM10.0197 9.67255L6.90644 8.54248C7.58164 7.51892 8.75184 6.87042 10.0197 6.87042C11.2875 6.87042 12.4577 7.51892 13.1329 8.54248L10.0197 9.67255ZM16.68 15.9703L10.8009 18.1044V11.0513L16.68 8.91724V15.9703Z" fill="currentColor"/>
                                                            </svg>

                                                        </div>
                                                        <div class="font-size-sm">5 lessons</div>
                                                    </div>
                                                </li>
                                                <li class="nav-item px-3">
                                                    <div class="d-flex align-items-center">
                                                        <div class="me-2 d-flex icon-uxs text-secondary">
                                                            <!-- Icon -->
                                                            <svg width="16" height="16" viewBox="0 0 16 16" xmlns="http://www.w3.org/2000/svg">
                                                                <path d="M14.3164 4.20996C13.985 4.37028 13.8464 4.76904 14.0067 5.10026C14.4447 6.00505 14.6667 6.98031 14.6667 8C14.6667 11.6759 11.6759 14.6667 8 14.6667C4.32406 14.6667 1.33333 11.6759 1.33333 8C1.33333 4.32406 4.32406 1.33333 8 1.33333C9.52328 1.33333 10.9543 1.83073 12.1387 2.77165C12.4259 3.00098 12.846 2.95296 13.0754 2.66471C13.3047 2.37663 13.2567 1.95703 12.9683 1.72803C11.5661 0.613607 9.8016 0 8 0C3.58903 0 0 3.58903 0 8C0 12.411 3.58903 16 8 16C12.411 16 16 12.411 16 8C16 6.77767 15.7331 5.60628 15.2067 4.51969C15.0467 4.18766 14.6466 4.04932 14.3164 4.20996Z" fill="currentColor"/>
                                                                <path d="M7.99967 2.66663C7.63167 2.66663 7.33301 2.96529 7.33301 3.33329V7.99996C7.33301 8.36796 7.63167 8.66663 7.99967 8.66663H11.333C11.701 8.66663 11.9997 8.36796 11.9997 7.99996C11.9997 7.63196 11.701 7.33329 11.333 7.33329H8.66634V3.33329C8.66634 2.96529 8.36768 2.66663 7.99967 2.66663Z" fill="currentColor"/>
                                                            </svg>

                                                        </div>
                                                        <div class="font-size-sm">8h 12m</div>
                                                    </div>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                <% }) %>
            </div>
        </div>
    </section>

    <!-- FOOTER
    ================================================== -->
	<%- include('partials/footer', { }) %>


    <!-- JAVASCRIPT
    ================================================== -->
    <!-- Libs JS -->
    <script src="/libs/jquery/dist/jquery.min.js"></script>
    <script src="/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="/libs/@fancyapps/fancybox/dist/jquery.fancybox.min.js"></script>
    <script src="/libs/aos/dist/aos.js"></script>
    <script src="/libs/choices.js/public/assets/scripts/choices.min.js"></script>
    <!-- <script src="/libs/countup.js/dist/countUp.min.js"></script> -->
    <script src="/libs/dropzone/dist/dropzone-min.js"></script>
    <script src="/libs/flickity/dist/flickity.pkgd.min.js"></script>
    <script src="/libs/flickity-fade/flickity-fade.js"></script>
    <script src="/libs/highlightjs/highlight.pack.min.js"></script>
    <script src="/libs/imagesloaded/imagesloaded.pkgd.min.js"></script>
    <script src="/libs/isotope-layout/dist/isotope.pkgd.min.js"></script>
    <script src="/libs/jarallax/dist/jarallax.min.js"></script>
    <script src="/libs/jarallax/dist/jarallax-video.min.js"></script>
    <script src="/libs/jarallax/dist/jarallax-element.min.js"></script>
    <script src="/libs/parallax-js/dist/parallax.min.js"></script>
    <script src="/libs/quill/dist/quill.min.js"></script>
    <script src="/libs/smooth-scroll/dist/smooth-scroll.min.js"></script>
    <script src="/libs/typed.js/dist/typed.umd.js"></script>

    <!-- Map -->
    <script src='https://api.mapbox.com/mapbox-gl-js/v0.53.0/mapbox-gl.js'></script>

    <!-- Theme JS -->
    <script src="/js/theme.min.js"></script>


</body>
</html>
